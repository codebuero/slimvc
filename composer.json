{
    "name": "zacao/slimvc",
    "description": "PHP Slim MVC Skeleton/Boilerplate Project",
    "keywords": ["slim", "mvc", "slimvc", "slim-mvc", "slimmvc", "router", "skeleton", "boilerplate"],
    "type": "project",
    "homepage": "https://github.com/zacao/slimvc",
    "license":"MIT",
    "autoload": {
        "psr-4": {
